<%= render layout: 'authorization_requests/shared/blocks/summary_block', locals: { authorization_request:, title: f.wording_for('steps.supporting_documents'), block_id: :supporting_documents, f:, editable: } do %>
  <% if authorization_request.respond_to?(:attestation_fiscale) %>
    <p>
      <strong><%= f.label_value(:attestation_fiscale) %> :</strong>
      <br />
      <% if authorization_request.attestation_fiscale.attached? %>
        <%= render 'shared/display_documents', documents: authorization_request.attestation_fiscale, f: f %>
      <% else %>
        Document manquant
      <% end %>
    </p>
  <% end %>
<% end %>
