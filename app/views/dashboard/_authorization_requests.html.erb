<h3 class="with-icon fr-mt-8w fr-mb-4w dashboard-title__panel">
  <%= image_tag("authorization_requests/#{kind}-icon.svg") %>
  <%= t(".#{kind}_title") %>
</h3>

<div class="fr-grid-row fr-grid-row--center fr-grid-row--middle">
  <% authorization_requests.each do |authorization_request| %>
    <div class="fr-col-12">
      <%= render partial: 'authorization_request_card', locals: { authorization_request: } %>
    </div>
  <% end %>
</div>

